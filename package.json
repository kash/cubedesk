{
	"name": "cubedesk",
	"version": "1.0.0",
	"description": "CubeDesk's codebase",
	"main": "server/app.ts",
	"scripts": {
		"build": "node esbuild.js",
		"server": "ts-node-dev --transpile-only --quiet --exit-child ./server/app.ts",
		"dev": "concurrently \"yarn build\" \"yarn server\" \"graphql-codegen --watch\"",
		"devs": "HTTPS=true concurrently \"yarn build\" \"yarn server\" \"graphql-codegen --watch\"",
		"dev2": "PORT=3001 yarn server",
		"test": "jest",
		"compile-server": "babel server --out-dir ./build/server --extensions \".ts,.js,.jsx,.tsx\" -s --config-file ./prod.babelrc",
		"compile-client": "babel client --out-dir ./build/client --extensions \".ts,.js,.jsx,.tsx\" -s --config-file ./prod.babelrc",
		"compile-shared": "babel shared --out-dir ./build/shared --extensions \".ts,.js,.jsx,.tsx\" -s --config-file ./prod.babelrc",
		"deploy": "graphql-codegen && yarn build && yarn compile-server && yarn compile-client && yarn compile-shared"
	},
	"engines": {
		"node": "16.*"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|scss|sass)$": "identity-obj-proxy"
		},
		"transform": {
			"node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!variables/.*)"
		],
		"setupFiles": [
			"./server/__test__/jest.setup.ts"
		]
	},
	"private": true,
	"author": "Kash Goudarzi",
